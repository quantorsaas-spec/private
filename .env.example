# Copy to .env and override as needed
POSTGRES_DB=quantor
POSTGRES_USER=quantor
POSTGRES_PASSWORD=quantor

# Quantor runtime
QUANTOR_MASTER_PASSWORD=change_me

# Observability (optional)
# By default tracing is disabled locally to avoid connection errors when no collector is running.
QUANTOR_TRACING_ENABLED=false
# Example: http://otel-collector:4317
QUANTOR_OTLP_ENDPOINT=
